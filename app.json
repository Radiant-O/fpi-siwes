{
  "expo": {
    "name": "fpi-siwes",
    "slug": "fpi-siwes",
    "scheme": "fpi-siwes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/sch-logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/sch-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "package": "com.radiant.siwes"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/sch-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.radiant.siwes"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8395214a-b9ee-4de0-8fd8-f7d22c9bd7c1"
      }
    }
  }
}
